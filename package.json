{
  "name": "port-plaisance-russell-api",
  "version": "1.0.0",
  "description": "API de gestion du port de plaisance de Russell",
  "main": "app.js",
  "scripts": {
    "start": "node bin/www",
    "dev": "nodemon bin/www",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "admin": "node scripts/Admin.js",
    "build": "node -e \"const fs=require('fs-extra');const path=require('path');const dirs=['src/models'];dirs.forEach(dir=>{const fullPath=path.join(process.cwd(),dir);if(!fs.existsSync(fullPath)){fs.mkdirpSync(fullPath)}});\"",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "dotenv/config"
    ]
  }
}