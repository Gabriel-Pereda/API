{
  "name": "port-plaisance-russell-api",
  "version": "1.0.0",
  "description": "API de gestion du port de plaisance de Russell",
  "scripts": {
    "start": "node bin/www",
    "dev": "nodemon bin/www",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "admin": "node scripts/Admin.js",
    "postinstall": "node -e \"const fs=require('fs'); const path=require('path'); const dir=path.join('src','models'); if(!fs.existsSync(dir)){fs.mkdirSync(dir,{recursive:true})}\""
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "dotenv/config"
    ]
  },
  "dependencies": {
    "axios": "^1.8.3",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "6.14",
    "mongoose": "^8.12.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0"
  },
  "engines": {
    "node": "18.x"
  }
}